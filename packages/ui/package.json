{
  "name": "@tactoui/ui",
  "version": "0.1.0",
  "description": "React Native UI component library with NativeWind styling",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "dependencies": {
    "@react-native-community/slider": "^5.0.1",
    "clsx": "^2.1.1",
    "nativewind": "^2.0.11",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-reanimated": "^3.10.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-native": "^0.73.0",
    "tsup": "^8.0.1",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "react-native",
    "ui",
    "components",
    "nativewind",
    "tailwind"
  ],
  "author": "TactoUI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tactoui/tactoui.git",
    "directory": "packages/ui"
  }
}
