{
  "name": "@tactoui/cli",
  "version": "0.1.0",
  "description": "CLI tool for adding TactoUI components to React Native projects",
  "main": "dist/index.js",
  "bin": {
    "tactoui": "dist/index.js"
  },
  "files": [
    "dist",
